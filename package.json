  },
  "license": "MIT",
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.10.1"
  },
  "scripts": {
    "analyze": "npm run build:with-stats && npm run start:analyzer",
    "build": "npx --no-install cross-env NODE_ENV=production node scripts/build.js",
    "build:dev": "npx --no-install cross-env NODE_ENV=development node scripts/build.js",
    "depgraph": "npm run build:depgraph && open-cli dependency-graph.svg",
    "build:depgraph": "depcruise -c .dependency-cruiser.js --exclude '^node_modules' --output-type dot src | dot -T svg > dependency-graph.svg",
    "clean": "npx rimraf --no-install build",
    "clean:transpiled": "npx rimraf --no-install build/transpiled",
    "i18n:scan": "npx --no-install cross-env NODE_ENV=i18n npm run transpile && i18next-scanner './build/transpiled/**/*.js' && npm run clean:transpiled",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:css\"",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "start:client": "npx --no-install cross-env NODE_ENV=development node scripts/start.js --client-only",
    "prestart": "npm run sync",
    "start": "npx --no-install cross-env NODE_ENV=development node scripts/start.js",
    "start:prod": "npx --no-install cross-env NODE_ENV=production node scripts/start.js",
    "storybook": "npm run storybook-serve",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test:ci": "npx --no-install cross-env CI=true node scripts/test.js --env=jsdom --coverage",
    "test:update": "npx --no-install cross-env CI=true npm test --updateSnapshot",
    "test:report": "open-cli ./coverage/lcov-report/index.html",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "transpile": "babel -d build/transpiled ./src --extensions .es6,.js,.es,.jsx,.mjs,.ts,.tsx --ignore **/*.d.ts",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix-js": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint:css": "stylelint src/**/*.css",
    "build:with-stats": "npx --no-install cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "build:storybook": "npm run build:lib && build-storybook",
    "build:storybook-html": "node ./scripts/build-storybook-html",
    "build:storybook-ts": "npx --no-install tsc -p tsconfig.storybook.json",
    "build:lib": "npm run build:storybook-ts && npm run copy-css-to-lib",
    "copy-css-to-lib": "npx --no-install cpx \"./node_modules/@atoll/shared/dist/index.cjs.css\" ./build/storybook",
    "prestorybook": "npm run build:lib && npm run build:storybook-html",
    "storybook-serve": "start-storybook -p 6006",
    "local-publish:shared": "cd .. && cd atoll-shared && npm run local-publish && cd ..",
    "local-link:shared": "npx --no-install yalc add @atoll/shared",
    "sync": "npm run local-publish:shared && npm run local-link:shared",
    "start:client-only": "npm run build:dev && npm run start:client"
  },
  "dependencies": {
    "@atoll/shared": "file:.yalc/@atoll/shared",
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "chalk": "2.4.2",
    "connected-react-router": "6.5.2",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-manifest-helpers": "0.6.0",
    "history": "4.9.0",
    "i18next": "19.0.0",
    "immer": "3.2.0",
    "pg": "7.14.0",
    "pg-hstore": "2.3.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-i18next": "11.2.1",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "sequelize": "5.21.2"
  },
