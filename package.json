  "scripts": {
    "analyze": "yarn-or-npm run build:with-stats && yarn-or-npm run start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "depgraph": "yarn-or-npm run build:depgraph && open-cli dependency-graph.svg",
    "build:depgraph": "depcruise -c .dependency-cruiser.js --exclude '^node_modules' --output-type dot src | dot -T svg > dependency-graph.svg",
    "clean": "yarn-or-npm run clean:transpiled",
    "clean:transpiled": "rimraf build/transpiled",
    "i18n:scan": "cross-env NODE_ENV=i18n yarn-or-npm run transpile && i18next-scanner './build/transpiled/**/*.js' && yarn-or-npm run clean:transpiled",
    "lint": "concurrently \"yarn-or-npm run lint:js\" \"yarn-or-npm run lint:css\"",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:prod": "cross-env NODE_ENV=production node scripts/start.js",
    "storybook": "yarn-or-npm run storybook-serve",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test:ci": "cross-env CI=true node scripts/test.js --env=jsdom --coverage",
    "test:update": "cross-env CI=true yarn-or-npm test --updateSnapshot",
    "test:report": "open-cli ./coverage/lcov-report/index.html",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "transpile": "babel -d build/transpiled ./src --extensions .es6,.js,.es,.jsx,.mjs,.ts,.tsx --ignore **/*.d.ts",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix-js": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "lint:css": "stylelint src/**/*.css",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "build:storybook": "npm run build:lib && build-storybook",
    "build:storybook-html": "node ./scripts/build-storybook-html",
    "build:storybook-ts": "tsc -p tsconfig.storybook.json",
    "build:lib": "npm run build:storybook-ts && npm run copy-css-to-lib",
    "copy-css-to-lib": "cpx \"./src/src/shared/components/**/*.css\" ./build/storybook",
    "prestorybook": "yarn-or-npm run build:lib && yarn-or-npm run build:storybook-html",
    "storybook-serve": "start-storybook -p 6006"
  },
